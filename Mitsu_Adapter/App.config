<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<sectionGroup name="mtcadapter" type="ConfigurationBase.PLCGroups,ConfigurationBase">
			<section name="mtsection" type="ConfigurationBase.GroupSection,ConfigurationBase" />
		</sectionGroup>
	</configSections>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
	</startup>
	<appSettings>
		<add key="ipAddress" value="192.168.1.254" />
		<add key="IdleTestTimeInMIN" value="5" />
		<add key="waitBeforeNextSendMS" value="2000" />
		<add key="debug" value="1" />
		<add key="adapterPort" value="7879" />
		<add key="PLCLogicalStation" value="1" />
		<add key="MachineType" value="Z31_DrumDetails" />
	</appSettings>
	<mtcadapter>
		
		<mtsection>
			<groups>
				<group name="SAMPLE">
					<elements>
						<param name="D0" paramSyntax="D0,2" convertion="real" />
					</elements>
				</group>
				<group name="EVENT">
					<elements>
						<param name="D0-10" paramSyntax="D0,10" convertion="real" />
						<param name="Y7" paramSyntax="Y0B8" convertion="real" />
						<param name="SubMotorCurrent" paramSyntax="D6321" convertion="real" />
					</elements>
				</group>
			</groups>
		</mtsection>
	</mtcadapter>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="System.Runtime.CompilerServices.Unsafe" publicKeyToken="b03f5f7f11d50a3a" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-6.0.0.0" newVersion="6.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Memory" publicKeyToken="cc7b13ffcd2ddd51" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.0.1.2" newVersion="4.0.1.2" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Text.Json" publicKeyToken="cc7b13ffcd2ddd51" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-7.0.0.2" newVersion="7.0.0.2" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Buffers" publicKeyToken="cc7b13ffcd2ddd51" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.0.3.0" newVersion="4.0.3.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>